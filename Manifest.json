  
{
    "name": "NS_Extension",
    "version": "1.1.0",
    "manifest_version": 2,
    "content_scripts":[
      {
        "matches":["<all_urls>", "desktop.cxengage.net/*"],
        "js":["content.js"],
        "run_at": "document_end"
      }
    ],
    "background":{
      "scripts": ["popup.js"],
      "persistent": false
    },
    "permissions": ["tabs","activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Ns_extensions",
      "default_icon": "icon.png"
    },
    "externally_connectable":{
      "matches": ["<all_urls>"]
    }
  }